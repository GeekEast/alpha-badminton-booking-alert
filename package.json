{
  "name": "badminton-booking-alarm",
  "version": "0.0.1",
  "description": "The Badminton Booking Alarm Service",
  "author": "james@sapia.ai",
  "private": true,
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "env-cmd -f .env.local nest start --watch",
    "start": "node dist/src/main",
    "start:debug": "env-cmd -f .env.local nest start --debug --watch --inspect-brk",
    "lint:fix": "eslint --fix",
    "prettier:fix": "prettier --write --config .prettierrc.js",
    "fmt": "pnpm lint:fix \"./**/*.{ts,js}\" && pnpm prettier:fix \"./**/*.{ts,js}\"",
    "test": "env-cmd -f .env.test jest --config ./jest/unit.config.ts --forceExit --runInBand --detectOpenHandles --passWithNoTests --no-caches",
    "tdd": "pnpm test -- --watch",
    "e2e": "env-cmd -f .env.test jest --config ./jest/e2e.config.ts --forceExit  --runInBand --detectOpenHandles  --passWithNoTests --no-caches",
    "test:clean": "jest --clearCache",
    "cmd": "env-cmd -f .env.local zx ./scripts/cmd.mjs",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@apollo/server": "4.10.2",
    "@apollo/subgraph": "2.7.2",
    "@aws-sdk/client-eventbridge": "3.549.0",
    "@nestjs/apollo": "12.1.0",
    "@nestjs/common": "^10.0.3",
    "@nestjs/core": "^10.0.3",
    "@nestjs/graphql": "12.1.0",
    "@nestjs/platform-express": "^10.0.3",
    "dynamoose": "3.2.1",
    "nestjs-dynamoose": "0.5.5",
    "axios": "1.6.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dayjs": "1.11.10",
    "fast-safe-stringify": "2.1.1",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-type-json": "0.3.2",
    "lodash": "4.17.21",
    "nestjs-cls": "4.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serialize-error": "8.1.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "2.1.0",
    "@aws-sdk/client-dynamodb": "3.465.0",
    "@aws-sdk/lib-dynamodb": "3.465.0",
    "@automock/jest": "2.1.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@golevelup/ts-jest": "0.4.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@nestjs/cli": "^10.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/deep-equal": "1.0.4",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "4.17.0",
    "@types/node": "^18.19.30",
    "@types/prettyjson": "0.0.33",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "deep-equal": "2.2.3",
    "env-cmd": "10.1.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.5",
    "prettyjson": "1.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "supertest-graphql": "1.1.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "zx": "8.0.1"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "pnpm lint:fix",
      "pnpm prettier:fix"
    ]
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.3",
    "pnpm": "8.15.5"
  }
}